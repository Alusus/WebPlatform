    }

    ماكرو عرب_خصلة[اسم، لاحقة_اسم، اسم_إنجليزي، name] {
        عرف اسم: لقب اسم_إنجليزي؛
        عرف هات__لاحقة_اسم__: لقب get__name__؛
        عرف حدد__لاحقة_اسم__: لقب set__name__؛
    }

    عرف إشـارة: لقب Signal؛
    @دمج صنف إشـارة {
